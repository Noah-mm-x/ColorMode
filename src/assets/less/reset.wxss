div,
p,
dl,
dd,
h1,
h2,
h3,
h4,
h5,
h6,
a,
b,
u,
em,
input,
textarea,
button,
select,
form,
section,
label,
span,
td,
li,
nav,
footer,
body,
ul,
i {
  -webkit-box-sizing: border-box;
  -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
  padding: 0;
  margin: 0;
}
ul,
li,
ol {
  list-style: none;
}
table {
  border-collapse: collapse;
}
body,
td,
input,
textarea,
option,
select,
button {
  -webkit-font-smoothing: antialiased;
  /*chrome„ÄÅsafari*/
  -moz-osx-font-smoothing: grayscale;
  /*firefox*/
  font-family: "Microsoft YaHei", helvetica, arial;
  -webkit-text-size-adjust: none;
}
html,
body,
td,
h3 {
  font-size: 14px;
}
html {
  width: 100%;
  height: 100%;
}
body {
  width: 100%;
  height: 100%;
  min-height: 100%;
  margin: 0 auto;
  background: #fff;
}
#app {
  min-height: 100%;
}
em,
i {
  font-style: normal;
}
p {
  -webkit-text-size-adjust: none !important;
}
body {
  background: #fff;
}
a {
  color: #333;
  text-decoration: none;
}
a:link,
a:visited {
  color: #333;
}
a,
img,
button {
  -webkit-touch-callout: none;
  -webkit-user-select: none;
}
a:focus,
a:active,
a:hover,
input,
button,
textarea {
  outline: none;
}
